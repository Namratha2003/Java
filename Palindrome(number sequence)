import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int num= sc.nextInt();
        int comp=num;
        int sum=0,rem=0;
        while(num>0){
            rem=num%10;
            sum=(sum*10)+rem;
            num=num/10;
        }
        if(sum==comp)
        System.out.println("Is a palindrome");
        else
        System.out.println("Is not a palindrome");
    }
}
