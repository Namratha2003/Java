import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num= sc.nextInt();
        int n=0,rem=0;
        while(num>0){
        rem = num%10;
        n=(n*10)+rem;
        num=num/10;}
        System.out.println(n);
    }
}
